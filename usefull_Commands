1. python -m venv ~/env
2. source ~/env/bin/activate
3. deactivate
4. pip install -r requirements.txt
5. django-admin startproject profiles_project .
6. python manage.py startapp profiles_api
7. python manage.py runserver 0.0.0.0:8000
8. python manage.py makemigrations profiles_api
9. python manage.py migrate
10. python manage.py createsuperuser
